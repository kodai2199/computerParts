<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" minHeight="800.0" minWidth="1000.0" scaleShape="false" styleClass="bg1" stylesheets="@app.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.GuiController">
   <children>
      <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="100.0" stylesheets="@app.css">
         <children>
            <Button mnemonicParsing="false" styleClass="std-button" text="Saved Builds">
               <tooltip>
                  <Tooltip text="Show the list of builds created in the past" />
               </tooltip></Button>
            <Button mnemonicParsing="false" styleClass="std-button" text="New Build" />
            <Button mnemonicParsing="false" styleClass="std-button" stylesheets="@app.css" text="Your profile" />
         </children>
         <padding>
            <Insets right="50.0" />
         </padding>
      </HBox>
      <ScrollPane fitToHeight="true" fitToWidth="true" styleClass="bg1" stylesheets="@app.css" VBox.vgrow="ALWAYS">
         <content>
            <TilePane alignment="TOP_CENTER" hgap="50.0" prefColumns="2" prefHeight="200.0" prefTileHeight="200.0" prefTileWidth="450.0" prefWidth="200.0" styleClass="bg2" stylesheets="@app.css" vgap="100.0">
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                        <Label text="CPU" />
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="200.0" />
                  <HBox prefHeight="100.0" prefWidth="200.0" />
                  <HBox prefHeight="100.0" prefWidth="200.0" />
                  <HBox prefHeight="100.0" prefWidth="200.0" />
                  <HBox prefHeight="100.0" prefWidth="200.0" />
               </children>
            </TilePane>
         </content></ScrollPane>
   </children>
</VBox>
